#!/bin/bash
export C_DEFAULT="\033[39m"
export BLACK="\033[30m"
export RED="\033[91m"
export GREEN="\033[92m"
export YELLOW="\033[93m"
export BLUE="\033[34m"
export MAGENTA="\033[95m"
export CYAN="\033[96m"

function colorprint() {
	echo -e "$1${C_DEFAULT}"
}

function println() {
	echo "$1"
}

function print() {
	echo -n "$1"
}

export -f colorprint
export -f println
export -f print
