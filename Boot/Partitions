# SOURCEABLE HERMES DATA PACK

# DO NOT EDIT THIS FILE!
# This datapack contains HERMES partition table.

cd "$(dirname "$0")/../../"
export ROOTFS="$PWD"

export SYSTEM="$ROOTFS/System"
export DATA="$ROOTFS/Data"
export LOGS="$DATA/Logs"
export CACHE="$DATA/Cache"
export STOPSIG="$CACHE/____STOPSIG____"
export RSTSIG="$CACHE/____RSTSIG____"

export BOOTREFUSE="$CACHE/bootrefuse"
export BOOTSECT="$SYSTEM/Boot"

export USERS="$DATA/Users"
